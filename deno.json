{
	"compilerOptions": {
	},
	"importMap": "./imports.json",
	"lint": {
		"rules": {
			"tags": ["recommended"],
			"include": [
				"default-param-last",
				"eqeqeq",
				"explicit-function-return-type"
			],
			"exclude": []
		}
	},
	"fmt": {
		"options": {
			"useTabs": true,
			"lineWidth": 80,
			"indentWidth": 2
		}
	},
	"tasks": {
		"lint:run": "deno lint --unstable src",
		"lint:watch": "deno lint --watch --unstable src",
		"fmt:run": "deno fmt --unstable src",
		"fmt:watch": "deno fmt --watch --unstable src"
	}
}
